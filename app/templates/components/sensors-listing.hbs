<table class="u-full-width">
    <thead>
        <tr>
            <th>Nom</th>
            <th>Dernière valeur</th>
            <th>Unité</th>
        </tr>
    </thead>
    <tbody>

        {{#each sensors as |sensor|}}
        <tr>
            <td>{{sensor.name}}</td>
            <td>{{sensor.last}}</td>
            <td>{{sensor.unit}}</td>
        </tr>
        {{/each}}

    </tbody>
</table>
